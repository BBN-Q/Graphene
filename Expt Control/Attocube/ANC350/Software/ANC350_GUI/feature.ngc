<Panel Name="ANC350Res Features" Icon="key16.xpm">

  <Widget Type="Box" Style="Flat" X="0" Y="0" Align="Top Left">
    <Widget Type="Box" Caption="Device Info" X="0" Y="0">
      <Widget Type="Label"  Caption="Serial Number"   X="0" Y="0"  Alignment="Left"/>
      <Widget Type="Label"  Caption="SYNC"            X="0" Y="1"  Alignment="Left"/>
      <Widget Type="Label"  Caption="Lockin"          X="0" Y="2"  Alignment="Left"/>
      <Widget Type="Edit"   Address="0x66F"           X="1" Y="0"  Width="2" Display="Serial" ReadOnly="1"/>
      <Widget Type="Check"  Address="0x3053"          X="1" Y="1"  Width="2" ReadOnly="1"/>
      <Widget Type="Check"  Address="0x3058"          X="1" Y="2"  Width="2" ReadOnly="1"/>
    </Widget>
  
    <Widget Type="Box" Caption="Option Upgrade" X="0" Y="1" >
      <Widget Type="Label"  Caption="Key"     X="0" Y="0" Alignment="Left"/>
      <Widget Type="Edit"   Address="0x18D"   X="1" Y="0" Width="2" Display="Hex"/>
      <Widget Type="Label"  Caption="Enter the upgrade key, press enter and restart the device"     X="0" Y="1" Width="3" />
    </Widget>
  </Widget>

</Panel>
